@model REDO.Models.Greeting
<!DOCTYPE HTML>
<html xmlns:th="http://www.thymeleaf.org">
<head>
    <title>Getting Started: Serving Web Content</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
</head>
<body>
    <p>
        @foreach (var hello in Model.hellos)
        {
            @hello <br />
        }
        Hello, @Model.Name, this site has been visited @Model.VisitCount times since last server start!
    </p>
</body>
</html>